      "package": "com.powerhouse0784.Waste2Wealth"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-secure-store",
      "expo-router",
      "expo-localization"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "9b5186b2-3885-4c66-a1b9-c99f08d6cdcd"
      }
    }
  }
